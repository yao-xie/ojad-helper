{
  "name": "OJAD Helper",
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "Helper for OJAD",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "contextMenus",
    "storage"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://www.gavo.t.u-tokyo.ac.jp/ojad/search/*"
      ]
    }
  ]
}
